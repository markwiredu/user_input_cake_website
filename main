    //datemethod
    public static void printSimpleDateFormat() {
        SimpleDateFormat formatter = new SimpleDateFormat(
                "yyyy.MM.dd - HH:mm:ss ");
        Date currentTime = new Date();
        System.out.println(formatter.format(currentTime));

    }


